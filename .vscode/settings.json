{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cssModules.camelCase": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "attr_quotes": "single"
    },
    "javascript": {
      "self_closing_tag": true
    }
  },
  "emmet.triggerExpansionOnTab": true,
  "git.autofetch": true,
  "prettier.requireConfig": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#108a79",
    "statusBarItem.hoverBackground": "#15b8a1",
    "statusBar.foreground": "#e7e7e7",
    "statusBar.border": "#108a79",
    "activityBar.activeBackground": "#15b8a1",
    "activityBar.activeBorder": "#f8e2fc",
    "activityBar.background": "#15b8a1",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f8e2fc",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#108a79",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.border": "#108a79",
    "titleBar.inactiveBackground": "#108a7999",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#108a79"
}

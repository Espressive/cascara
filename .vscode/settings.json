{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cssModules.camelCase": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "attr_quotes": "single"
    },
    "javascript": {
      "self_closing_tag": true
    }
  },
  "emmet.triggerExpansionOnTab": true,
  "git.autofetch": true,
  "prettier.requireConfig": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#C51653",
    "activityBar.activeBorder": "#e7e7e7",
    "activityBar.background": "#B4104B",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#e7e7e7",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#90023A",
    "statusBar.border": "#90023A",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#90023A",
    "titleBar.activeBackground": "#90023A",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.border": "#90023A",
    "titleBar.inactiveBackground": "#90023A99",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
